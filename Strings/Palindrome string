https://www.geeksforgeeks.org/problems/palindrome-string0817/1

class Solution{
public:	
	
	
	int isPalindrome(string S)
	{
	    // Your code goes here
	    int i=0, j= S.size()-1;
	    while(i<j){
	        if( S[i]!= S[j]){
	            return false;
	            
	        }
	        else{
	            i++;
	            j--;
	        }
	    }
	    return true;
	}

};
