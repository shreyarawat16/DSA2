https://leetcode.com/submissions/detail/1330898992/

class Solution {
public:
int solve(int ind, int dp[46]){
    if(ind==0 || ind==1){
        return 1;
    }
    if(dp[ind]!=-1){
        return dp[ind];
    }
    else{
    dp[ind]= solve(ind-1, dp)+ solve(ind-2, dp);
    }
    return dp[ind];
}
    int climbStairs(int n) {
       int dp[46];
       for(int i=1; i<=45; i++){
         dp[i]=-1;
       }
       return solve(n, dp);
    }
};
